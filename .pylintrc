[MASTER]
init-hook='import sys; sys.path.append(".")'

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    fixme

[FORMAT]
max-line-length=88
good-names=i,j,k,ex,Run,_,id

[DESIGN]
max-args=7
max-locals=15
max-statements=50

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes